{
  "name": "kettle-ui",
  "version": "1.0.0",
  "description": "material-ui based widgets",
  "main": "index.js",
  "scripts": {
    "clean": "mkdir -p example/dist && rm -rf example/dist/*",
    "indexpage": "cp example/index.html example/dist/index.html",
    "assets": "npm run indexpage",
    "watch": "WEBPACK_ARGS=--watch npm run webpack",
    "prewebpack": "npm run compile && npm run clean && npm run assets",
    "webpack": "webpack $WEBPACK_ARGS",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "prebuild": "npm run compile",
    "build": "NODE_ENV=development npm run webpack",
    "release": "NODE_ENV=production npm run webpack",
    "pretest": "npm run compile",
    "test": "mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binocarlos/kettle-ui.git"
  },
  "keywords": [
    "redux",
    "material",
    "react"
  ],
  "author": "Kai Davenport",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binocarlos/kettle-ui/issues"
  },
  "homepage": "https://github.com/binocarlos/kettle-ui",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "biro": "^3.0.1",
    "biro-material-ui": "^2.1.0",
    "expect": "^1.14.0",
    "jquery": "^3.0.0",
    "jsdom": "^8.1.0",
    "material-ui": "^0.15.1",
    "mocha": "^2.4.5",
    "react": "^15.1.0",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.0",
    "react-shallow-testutils": "^1.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.1.6",
    "webpack": "^1.12.12"
  }
}
