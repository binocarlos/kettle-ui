{
  "name": "kettle-ui",
  "version": "3.6.3",
  "description": "material-ui based widgets",
  "main": "index.js",
  "scripts": {
    "clean": "mkdir -p example/dist && rm -rf example/dist/*",
    "assets": "cp example/*.{html,css} example/dist",
    "watch": "NODE_ENV=development WEBPACK_ARGS=--watch npm run webpack",
    "prewebpack": "npm run clean && npm run assets",
    "webpack": "webpack $WEBPACK_ARGS",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "build": "NODE_ENV=development npm run webpack",
    "release": "NODE_ENV=production npm run webpack",
    "pretest": "npm run compile",
    "test": "mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binocarlos/kettle-ui.git"
  },
  "keywords": [
    "redux",
    "material",
    "react"
  ],
  "author": "Kai Davenport",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binocarlos/kettle-ui/issues"
  },
  "homepage": "https://github.com/binocarlos/kettle-ui",
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "dependencies": {
    "biro": "^3.2.5",
    "biro-material-ui": "^2.2.5",
    "material-ui": "^0.16.4"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "expect": "^1.14.0",
    "jquery": "^3.0.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "webpack": "^1.12.12",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.5",
    "react-shallow-testutils": "^2.0.0",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.5.2",
    "react": "^15.4.1"
  }
}
